{{> navigation}}

<div class="feed">
  <div class="artist-overview">
    <div class="aritst-pic-container">
      <img class="artist-pic" src="{{{ artist.artist_image }}}">
    </div>
    <div class="artist-info">
      <div class="artist-name">{{{ artist.artist_name }}}</div>
      <p class="artist-genre"><b>Genres</b></p>
      <div class="tags-area">
{{#each artist.genres}}
        <div class="tag">
          <p>{{this}}</p>
        </div>
{{/each}}
      </div>
      <br>
      <p class="artist-bio"><b>Biography</b></p>
      <p class="artist-bio">{{ artist.biography }} <a href="FullBiography-{{ artist.artist_name }}.html">read more</a></p>
    </div>
  </div>
  <div class="discog-header-container">
    <div class="discog-header">
      <h1>Discography</h1>
      <div class="discog-header-details">
        <p class="title-header"> Title / Release Date</p>
        <div class="empty-box"></div>
        <div class="empty-box"></div>
        <div class="empty-box"></div>
        <div class="empty-box"></div>
        <div class="empty-box"></div>
        <div class="empty-box"></div>
        <div class="empty-box"></div>
        <p class="reviews-header">Reviews</p>
        <p class="reviews-header">Average</p>
      </div>
    </div>
  </div>
     
</div>